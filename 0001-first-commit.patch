+from nltk.corpus import wordnet
+temp='program'
+
+syns=wordnet.synsets(temp)
+print(syns)
+print(syns[len(syns)-1].definition())
+#print(syns['program'])
+print(syns[0])
+print(syns[0].name())
+print(syns[0].lemmas())
